{
  "name": "ngx-malihu-scrollbar-angular18",
  "version": "1.0.1",
  "description": "Angular scrollbar customization using Malihu jQuery Custom Scrollbar plugin",
  "homepage": "https://github.com/Banuyaswanth/ngx-malihu-scrollbar-angular18",
  "license": "MIT",
  "author": {
    "name": "Jean-Francois Cere",
    "email": "jfcere@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Banuyaswanth/ngx-malihu-scrollbar-angular18"
  },
  "keywords": [
    "angular",
    "ngx",
    "malihu",
    "scrollbar"
  ],
  "scripts": {
    "ng": "ng",
    "start": "yarn link:lib && ng serve",
    "build:demo": "ng build demo",
    "build:lib": "ng build lib --configuration production",
    "postbuild:lib": "cpx ./README.md ./dist/lib && cpx ./LICENSE ./dist/lib",
    "link:lib": "rimraf node_modules/ngx-malihu-scrollbar && linklocal",
    "lint": "yarn lint:lib && yarn lint:demo",
    "lint:demo": "ng lint demo",
    "lint:lib": "ng lint lib",
    "lint:ci": "ng lint lib --format checkstyle > tslint.xml",
    "test": "ng test",
    "coveralls": "cat \"./coverage/lcov.info\" | \"./node_modules/coveralls/bin/coveralls.js\"",
    "gh-pages:build": "yarn build:demo --prod --aot --base-href /ngx-malihu-scrollbar/",
    "gh-pages:deploy": "angular-cli-ghpages --dir=\"dist/demo\" --name=\"CircleCI\" --email=\"circleci@email.com\" --no-silent",
    "publish:lib": "npm publish ./dist/lib"
  },
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "ngx-malihu-scrollbar": "file:./lib",
    "rxjs": "7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.13",
    "@angular/language-service": "^18.2.12",
    "@types/jasmine": "5.1.5",
    "@types/jasminewd2": "2.0.13",
    "@types/node": "22.10.10",
    "angular-cli-ghpages": "2.0.3",
    "codelyzer": "6.0.2",
    "coveralls": "3.1.1",
    "cpx": "^1.5.0",
    "jasmine-core": "5.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "karma-junit-reporter": "^1.2.0",
    "linklocal": "^2.8.2",
    "ng-packagr": "^18.2.1",
    "rimraf": "^6.0.1",
    "ts-node": "10.9.2",
    "tslint": "~6.1.3",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "5.4.4"
  }
}
